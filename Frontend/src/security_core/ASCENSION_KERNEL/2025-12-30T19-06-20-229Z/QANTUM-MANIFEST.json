  "architecture": {
    "entryPoint": "src/index.ts",
    "outputDir": "dist",
    "modules": {
      "core": {
        "path": "src/core",
        "description": "Quantum Core Engine - Main API",
        "exports": ["QAntum", "createQA", "SelfHealingEngine", "DeepSearchEngine"],
        "dependsOn": []
      },
      "healing": {
        "path": "src/healing",
        "description": "Self-Healing AI - Autonomous locator recovery",
        "exports": ["HealingReporter", "SelfHealingEngine"],
        "dependsOn": ["core"]
      },
      "ghost": {
        "path": "src/ghost",
        "description": "Ghost Protocol - Zero-detection stealth",
        "exports": ["GhostEngine", "WebGLMutator", "BiometricJitter", "TLSRotator"],
        "dependsOn": ["core"]
      },
      "swarm": {
        "path": "src/swarm",
        "description": "Swarm Orchestration - Distributed execution",
        "exports": ["SwarmOrchestrator", "ResultAggregator"],
        "dependsOn": ["core", "queue"]
      },
      "cognitive": {
        "path": "src/cognitive",
        "description": "Neural Intelligence - AI brain",
        "exports": ["AutonomousExplorer", "NeuralMapEngine", "AutoTestFactory"],
        "dependsOn": ["core", "ai"]
      },
      "precog": {
        "path": "src/pre-cog",
        "description": "Predictive Analytics - Anticipate failures",
        "exports": ["PreCogEngine"],
        "dependsOn": ["cognitive", "metrics"]
      },
      "fortress": {
        "path": "src/fortress",
        "description": "Production Isolation - Enterprise security",
        "exports": ["FortressMode"],
        "dependsOn": ["core", "security"]
      },
      "singularity": {
        "path": "src/singularity",
        "description": "Quantum Cryptography - Future-proof security",
        "exports": ["SingularityEngine"],
        "dependsOn": ["core"]
      },
      "visual": {
        "path": "src/visual",
        "description": "Visual Testing - Screenshot comparison",
        "exports": ["VisualTesting"],
        "dependsOn": ["core"]
      },
      "security": {
        "path": "src/security",
        "description": "Security Testing - XSS, SQLi, CSRF",
        "exports": ["SecurityTesting"],
        "dependsOn": ["core"]
      },
      "performance": {
        "path": "src/performance",
        "description": "Performance Testing - Web Vitals",
        "exports": ["PerformanceTesting"],
        "dependsOn": ["core", "metrics"]
      },
      "api": {
        "path": "src/api",
        "description": "REST API Server",
        "exports": ["APIServer"],
        "dependsOn": ["core"]
      },
      "cli": {
        "path": "src/cli",
        "description": "Command Line Interface",
        "exports": ["CLI"],
        "dependsOn": ["core"]
      },
      "integrations": {
        "path": "src/integrations",
        "description": "Third-party integrations",
        "exports": ["JiraIntegration", "SlackIntegration", "TestRailIntegration"],
        "dependsOn": ["core", "api"]
      },
      "metrics": {
        "path": "src/metrics",
        "description": "Prometheus compatible metrics",
        "exports": ["MetricsCollector"],
        "dependsOn": ["core"]
      },
      "maintenance": {
        "path": "src/maintenance",
        "description": "System health & self-repair",
        "exports": ["SystemMeditate", "DriftDetector", "AutoPatcher"],
        "dependsOn": ["core", "healing"]
      }
    }
  },

  "commands": {
    "cli": {
      "primary": "qantum",
      "alias": "qa"
    },
    "scripts": {
      "build": "npm run build",
      "test": "npx qantum run",
      "ghost": "npm run ghost:init",
      "swarm": "npm run swarm:run",
      "health": "npm run system:meditate"
    }
  },

  "patterns": {
    "naming": {
      "classes": "PascalCase",
      "functions": "camelCase",
      "files": "kebab-case.ts",
      "constants": "SCREAMING_SNAKE_CASE"
    },
    "imports": {
      "style": "named",
      "extensions": ".js"
    },
    "exports": {
      "style": "named",
      "reExportFromIndex": true
    }
  },

  "globalReplacements": {
    "deprecated": {
      "QAntum": "QAntum",
      "qantum": "qantum",
      "QANTUM": "QAntum",
      "QANTUM": "QANTUM",
      "createMM": "createQA",
      "mm.": "qa.",
      "QANTUM Team": "QAntum Labs"
