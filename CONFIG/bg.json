{
    "_copyright": "© 2025 Димитър Продромов (Dimitar Prodromov). All Rights Reserved.",
    "_version": "23.0.0",
    "_description": "Bulgarian Semantic Command Mapping for QAntum Voice Interface",
    
    "language": "bg",
    "locale": "bg-BG",
    "name": "Български",
    
    "commands": {
        "search": {
            "keywords": ["търси", "намери", "потърси", "търсене", "където"],
            "patterns": [
                "търси {query}",
                "намери {query}",
                "потърси {query}",
                "търсене за {query}",
                "където има {query}"
            ],
            "examples": [
                "търси бутон за вход",
                "намери текстовото поле",
                "потърси грешката"
            ]
        },
        
        "click": {
            "keywords": ["кликни", "натисни", "избери", "кликване", "щракни"],
            "patterns": [
                "кликни {target}",
                "натисни {target}",
                "избери {target}",
                "кликни върху {target}",
                "щракни на {target}"
            ],
            "examples": [
                "кликни бутона",
                "натисни Изпрати",
                "избери опцията"
            ]
        },
        
        "assert": {
            "keywords": ["провери", "увери", "потвърди", "валидирай", "очаквай"],
            "patterns": [
                "провери {condition}",
                "увери се че {condition}",
                "потвърди {condition}",
                "валидирай {condition}",
                "очаквай {condition}"
            ],
            "examples": [
                "провери дали текстът е видим",
                "увери се че бутонът е активен",
                "потвърди успешен вход"
            ]
        },
        
        "navigate": {
            "keywords": ["отиди", "навигирай", "отвори", "зареди", "премини"],
            "patterns": [
                "отиди на {url}",
                "навигирай към {url}",
                "отвори {url}",
                "зареди {url}",
                "премини към {url}"
            ],
            "examples": [
                "отиди на началната страница",
                "отвори настройките",
                "навигирай към профила"
            ]
        },
        
        "type": {
            "keywords": ["напиши", "въведи", "напечатай", "попълни", "постави"],
            "patterns": [
                "напиши {text}",
                "въведи {text}",
                "напечатай {text}",
                "попълни {text}",
                "постави {text}"
            ],
            "examples": [
                "напиши потребителско име",
                "въведи паролата",
                "попълни формата"
            ]
        },
        
        "wait": {
            "keywords": ["изчакай", "почакай", "чакай", "пауза"],
            "patterns": [
                "изчакай {duration}",
                "почакай {duration}",
                "чакай {duration} секунди",
                "пауза {duration}"
            ],
            "examples": [
                "изчакай 5 секунди",
                "почакай зареждането",
                "пауза 2 секунди"
            ]
        },
        
        "scroll": {
            "keywords": ["скролни", "превърти", "скролиране", "скрол"],
            "patterns": [
                "скролни {direction}",
                "превърти {direction}",
                "скрол {direction}",
                "скролни до {target}"
            ],
            "examples": [
                "скролни надолу",
                "превърти до края",
                "скрол до бутона"
            ]
        },
        
        "screenshot": {
            "keywords": ["снимка", "скрийншот", "екранна снимка", "запази"],
            "patterns": [
                "направи снимка",
                "скрийншот",
                "екранна снимка",
                "запази екрана"
            ],
            "examples": [
                "направи снимка на страницата",
                "скрийншот на грешката"
            ]
        },
        
        "hover": {
            "keywords": ["задържи", "покажи", "ховър"],
            "patterns": [
                "задържи върху {target}",
                "ховър на {target}",
                "покажи менюто"
            ],
            "examples": [
                "задържи върху иконата",
                "ховър на линка"
            ]
        },
        
        "drag": {
            "keywords": ["влачи", "премести", "дръпни", "плъзни"],
            "patterns": [
                "влачи {source} към {target}",
                "премести {source} до {target}",
                "плъзни {source}"
            ],
            "examples": [
                "влачи файла към папката",
                "премести елемента надясно"
            ]
        },
        
        "select": {
            "keywords": ["избери", "маркирай", "селектирай"],
            "patterns": [
                "избери {option}",
                "маркирай {option}",
                "селектирай {option}"
            ],
            "examples": [
                "избери от падащото меню",
                "маркирай текста"
            ]
        },
        
        "clear": {
            "keywords": ["изчисти", "изтрий", "премахни"],
            "patterns": [
                "изчисти {target}",
                "изтрий {target}",
                "премахни {target}"
            ],
            "examples": [
                "изчисти полето",
                "изтрий текста"
            ]
        },
        
        "refresh": {
            "keywords": ["опресни", "презареди", "рефреш"],
            "patterns": [
                "опресни страницата",
                "презареди",
                "рефреш"
            ],
            "examples": [
                "опресни страницата",
                "презареди данните"
            ]
        },
        
        "back": {
            "keywords": ["назад", "върни", "предишна"],
            "patterns": [
                "назад",
                "върни се",
                "предишна страница"
            ],
            "examples": [
                "върни се назад",
                "предишна страница"
            ]
        },
        
        "forward": {
            "keywords": ["напред", "следваща"],
            "patterns": [
                "напред",
                "следваща страница"
            ],
            "examples": [
                "отиди напред",
                "следваща страница"
            ]
        }
    },
    
    "entities": {
        "directions": {
            "up": ["нагоре", "горе"],
            "down": ["надолу", "долу"],
            "left": ["наляво", "вляво"],
            "right": ["надясно", "вдясно"],
            "top": ["в началото", "горе"],
            "bottom": ["в края", "долу"]
        },
        
        "elements": {
            "button": ["бутон", "бутона", "копче"],
            "link": ["линк", "връзка", "хипервръзка"],
            "input": ["поле", "текстово поле", "инпут"],
            "checkbox": ["чекбокс", "отметка", "квадратче"],
            "dropdown": ["падащо меню", "дропдаун", "списък"],
            "image": ["картинка", "изображение", "снимка"],
            "text": ["текст", "съдържание"],
            "menu": ["меню", "менюто"],
            "modal": ["модал", "диалог", "прозорец"],
            "table": ["таблица", "таблицата"],
            "form": ["форма", "формата", "формуляр"]
        },
        
        "states": {
            "visible": ["видим", "видима", "показан", "показана"],
            "hidden": ["скрит", "скрита", "невидим"],
            "enabled": ["активен", "активна", "включен"],
            "disabled": ["неактивен", "неактивна", "изключен"],
            "selected": ["избран", "избрана", "маркиран"],
            "checked": ["отметнат", "отметната", "чекнат"],
            "focused": ["фокусиран", "фокусирана"],
            "empty": ["празен", "празна"],
            "filled": ["попълнен", "попълнена"]
        },
        
        "timeUnits": {
            "seconds": ["секунди", "секунда", "сек"],
            "milliseconds": ["милисекунди", "мс"],
            "minutes": ["минути", "минута", "мин"]
        }
    },
    
    "modifiers": {
        "not": ["не", "без", "няма"],
        "and": ["и", "също"],
        "or": ["или"],
        "then": ["след това", "после", "тогава"],
        "if": ["ако", "когато"],
        "all": ["всички", "всичко"],
        "first": ["първи", "първия", "първият"],
        "last": ["последен", "последния", "последният"]
    },
    
    "confirmations": {
        "yes": ["да", "добре", "окей", "ок", "точно"],
        "no": ["не", "отказ", "стоп", "спри"]
    },
    
    "errorMessages": {
        "notFound": "Елементът не е намерен: {target}",
        "timeout": "Времето за изчакване изтече: {duration}ms",
        "invalidCommand": "Невалидна команда: {command}",
        "parseError": "Не мога да разбера командата: {input}"
    },
    
    "successMessages": {
        "clicked": "Успешно кликнато: {target}",
        "typed": "Текстът е въведен: {text}",
        "navigated": "Навигирано към: {url}",
        "asserted": "Проверката премина: {condition}",
        "waited": "Изчаках: {duration}"
    }
}
